{
	"manifest_version": 2,
	"name": "drop-down",
	"version": "1.0",
	"icons": {"48": "48x48.png"},
	"permissions": [
		"tabs", "*://admin-WINERY.ewinerysolutions.com/wineries/store/*"
	],
	
	"content_scripts": [
    {
		"matches": [ "*://admin-WINERY.ewinerysolutions.com/wineries/store/*"],
		"js": [ "ew_include.js" ],
		"run_at": "document_end"
	}
	],
	"web_accessible_resources": ["ew_script.js" ],

	"browser_action": {
		"default_title": "Modify Order Source Field",
		"default_icon": "48x48.png"
	}
 }